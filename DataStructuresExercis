//1
const jedi = [];
jedi[0] = "Luke";
jedi.push("Obi-Wan Kenobi");
console.log(jedi);
jedi.unshift("Yoda");
console.log(jedi[1]);
jedi.pop();
jedi.shift();
console.log(jedi);

//2
