//1
const jedi = [];
jedi[0] = "Luke";
jedi.push("Obi-Wan Kenobi");
console.log(jedi);
jedi.unshift("Yoda");
console.log(jedi[1]);
jedi.pop();
jedi.shift();
console.log(jedi);

//2
const imperialOfficers = ["Grand Moff Tarkin", "Orson Krennic"];
const sithLords = ["Darth Maul", "Darth Vader", "Emperor Palpatine"];
const starWarsVillains = sithLords.concat(imperialOfficers);
console.log(starWarsVillains); 
const selectedVillains = starWarsVillains.slice(0, 4); 
console.log(selectedVillains);

